{
    "version": 2,
    "builds": [
      {
        "src": "src/index.tsx", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "src/index.tsx" } 
    ],
    "functions": {
      "src/index.tsx": {
        "runtime": "vercel-node@0.9.1" 
      }
    }
  }
  